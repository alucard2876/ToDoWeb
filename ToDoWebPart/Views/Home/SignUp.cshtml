
@{
    ViewData["Title"] = "SignUp";
}

<h1>SignUp</h1>
<div>
    <form method="post" asp-action="SignUp">
        <input type="text" name="UserName"/>
        <input type="password" name="Password"/>
        <input type="password" name="ConfirmPassword"/>
        <input type="submit" value="Sign In"/>
    </form>
</div>

